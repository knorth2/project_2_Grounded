<header class="indexHeader">
  <%- include('./partials/header.ejs') %>
  <h1 class="h1Index">Calendar of Events</h1>
  <a href="/mindful/new">Add a *new Post</a>
  
</header>



<ul class="indexContainer"> 
  <% for(let i = 0; i < mindful.length; i++){ %>
   
  <div class="indexItems">
    <div id="indexDate"> <%= mindful[i].date %></div>
    <a href="/mindful/<%= mindful[i].id %>">
      <img class="indexImage" src="<%=mindful[i].image%>" />
    </a> 
  </div>
  <% } %>
</ul>

<footer class="indexFooter"><%- include('./partials/footer.ejs') %></footer>
